{
    "name": "greybee/choredo",
    "description": "API For ChoreDo",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1",
        "beberlei/assert": "^2.7",
        "doctrine/migrations": "^1.5",
        "doctrine/orm": "^2.5",
        "league/container": "^2.4",
        "league/route": "^3.0",
        "monolog/monolog": "^1.23",
        "ramsey/uuid": "^3.7",
        "ramsey/uuid-doctrine": "^1.4",
        "vlucas/phpdotenv": "~2.2",
        "league/fractal": "^0.17.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~6.0",
        "mockery/mockery": "~0.9",
        "squizlabs/php_codesniffer": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Choredo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Choredo\\Test\\": "test/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
